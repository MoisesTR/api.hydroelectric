    "awilix": "^4.2.6",
    "awilix-express": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "mssql": "^6.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
